    "   \"metadata\": {},\n",
    "   \"outputs\": [\n",
    "    {\n",
    "     \"data\": {\n",
    "      \"text/plain\": [\n",
    "       \"ChatCompletionMessageToolCall(id='call_0_8483817a-a35b-4718-ab8a-1046b3cfc1de', function=Function(arguments='{\\\"location\\\":\\\"Shanghai\\\"}', name='get_weather'), type='function', index=0)\"\n",
